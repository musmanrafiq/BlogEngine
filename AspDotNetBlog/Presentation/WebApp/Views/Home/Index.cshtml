@model IEnumerable<BlogEngine.Business.Models.PostModel>


<h1 class="title">Latest</h1>
<hr />
@foreach (var post in Model)
{
    <div class="card">
        <div class="card-content">
            <p class="title is-5">
                @post.Title
            </p>
            <p class="subtitle is-6">
                @post.Content
            </p>
        </div>
    </div>
}
